{
  "compilerOptions": {
    "outDir": "DocJs",                    /* Skip type checking .d.ts files that are included with TypeScript. */
    "target": "es2016",   
    "module": "commonjs",                         /* Allow 'import x from y' when a module doesn't have a default export. */
    "esModuleInterop": true,                         /* Disable resolving symlinks to their realpath. This correlates to the same flag in node. */
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "skipLibCheck": true 
  },
  "files": [
    "DocTs/test.ts",
    "DocTs/test1.ts",
    "DocTs/test2.ts"
  ]
}
